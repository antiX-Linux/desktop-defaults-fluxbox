[begin] (--antiX--)
    [exec] (Terminal) {desktop-defaults-run -t}
    [exec] (Acasă) {desktop-defaults-run -fm}
    [separator]
    [exec] (Navigator) {desktop-defaults-run -b}
    [exec] (Editor) {desktop-defaults-run -te}
    [separator]
    [submenu] (Personal)
        [begin] (Personal)
        [include] (~/.fluxbox/personal)
    [end]
    [submenu] (Aplicatii)
        [begin] (Aplicatii)
        [include] (~/.fluxbox/menu-applications)
    [end]
    [submenu] (Update Menus)
        [exec] (Update Personal Menu) {desktop-menu --menu-file='/etc/xdg/menus/TCM-MENU.menu' --write-out --write-out-file='personal'}
        [exec] (Update Local App Menu) {desktop-menu --write-out}
        [exec] (Update Global App Menu) {desktop-menu --write-out-global}
    [end]
    [separator]
    [submenu] (Birou)
        [submenu] (Alternate Desktops)
            [begin] (Alternate Desktops)
            [include] (/usr/share/desktop-session/wm-menus/fluxbox-wm-menu)
        [end]
        [separator]
        [exec] (Add Desktop) {add-desktop}
        [exec] (Wallpaper) {wallpaper.py}
        [exec] (GTK Theme) {lxappearance}
        [exec] (Desktop Defaults) {desktop-defaults-set}
        [exec] (Window Manager) {edit-wm-settings}
        [exec] (Session Settings) {desktop-defaults-run -te ~/.desktop-session/desktop-session.conf}
        [exec] (Session Settings Global) {desktop-defaults-run -r -te /etc/desktop-session/desktop-session.conf}
        [separator]
        [exec] (Panou Rox on/off) {paneltoggle.sh}
        [exec] (Conky on/off) {conkytoggle.sh}
    [end]
    [separator]
    [submenu] (Unelte)
        [exec] (Nano) {desktop-defaults-run -t nano}
        [exec] (Ted) {/usr/bin/Ted}
        [exec] (Rox Filer) {rox}
        [exec] (SpaceFM) {spacefm}
        [exec] (Caută) {spacefm --find-files}
        [exec] (MC File Manager) {desktop-defaults-run -t mc}
        [exec] (Termină) {xkill}
        [exec] (Vizualizare procese) {desktop-defaults-run -t htop}
        [exec] (Sistemul de Informaţii) {inxi-gui}
        [exec] (Adăugare fișiere .desktop) {ktsuss adddesktop.sh}
    [end]
    [exec] (Centru de Control) {antixcc.sh}
    [separator]
    [submenu] (Ajutor)
        [submenu] (antiX)
            [exec] (Întrebări frecvente) {dillo /usr/share/antiX/FAQ/index.html}
            [exec] (Live Ghid) {dillo /usr/share/antiX/Boot_Menu/antiX-gfxboot.html}
            [separator]
            [exec] (Manual Rox) {desktop-defaults-run -b http://rox.sourceforge.net/Manual/Manual/Manual.html}
        [end]
        [separator]
        [submenu] (Fluxbox)
            [exec] (Documente) {desktop-defaults-run -b http://fluxbox.sourceforge.net/docbook/en/html/}
            [exec] (Întrebări frecvente) {desktop-defaults-run -b http://fluxbox-wiki.org/index.php/Faqs}
            [exec] (Fluxbox Wiki) {desktop-defaults-run -b http://fluxbox-wiki.org/index.php/Fluxbox-wiki}
            [separator]
            [exec] (Arch Wiki) {desktop-defaults-run -b http://wiki.archlinux.org/index.php/Fluxbox}
        [end]
        [submenu] (IceWM)
            [exec] (Întrebări frecvente) {desktop-defaults-run -b http://www.icewm.org/FAQ/}
            [exec] (Manual IceWM) {desktop-defaults-run -b http://www.icewm.org/manual/}
            [separator]
            [exec] (OSNews) {desktop-defaults-run -b http://www.osnews.com/story/7774/IceWM--The-Cool-Window-Manager/}
        [end]
        [submenu] (JWM)
            [exec] (Homepage) {desktop-defaults-run -b http://joewing.net/programs/jwm/}
            [exec] (Arch Wiki) {desktop-defaults-run -b https://wiki.archlinux.org/index.php/JWM}
            [exec] (WM - Puppy-Linux) {desktop-defaults-run -b http://puppy.b0x.me/wiki/doku.php?id=jwm}
        [end]
        [separator]
        [submenu] (Aplicatii)
            [exec] (Firewall) {desktop-defaults-run -b https://help.ubuntu.com/community/Gufw}
            [exec] (mtPaint) {desktop-defaults-run -b http://mtpaint.sourceforge.net/handbook/}
            [exec] (Partimage) {desktop-defaults-run -b http://www.partimage.org/Partimage-manual_Usage}
        [end]
        [submenu] (Manual)
            [exec] (Backup) {desktop-defaults-run -t man luckybackup}
            [exec] (Irssi) {desktop-defaults-run -t man irssi}
            [exec] (mc) {desktop-defaults-run -t man mc}
            [exec] (mocp) {desktop-defaults-run -t man mocp}
            [exec] (Nano) {desktop-defaults-run -t man nano}
            [exec] (Rss/Atom) {desktop-defaults-run -t man newsbeuter}
            [exec] (Terminal) {desktop-defaults-run -t man urxvt}
            [exec] (Torrent) {desktop-defaults-run -t man rtorrent}
        [end]
    [end]
    [exec] (Rulează) {gexec}
    [submenu] (Fluxbox)
        [exec] (Update Local Menu) {desktop-menu --write-out}
        [exec] (Update global Menu) {desktop-menu --write-out-global}
        [separator]
        [config] (Configurație)
        [submenu] (Stiluri)
            [stylesdir] (~/.fluxbox/styles)
            [stylesmenu] (Stiluri Fluxbox) {/usr/share/fluxbox/styles}
        [end]
        [exec] (Tapete) {wallpaper.py}
        [separator]
        [workspaces] (Spații de lucru)
        [separator]
        [reconfig] (Reconfigurează)
        [restart] (Repornește)
    [end]
    [exec] (Ieșire) {desktop-session-exit}
    [exec] (Instalează) {gksu antixsources.sh}
[end]
